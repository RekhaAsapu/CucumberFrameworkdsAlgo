<suite name="Combined Suite">
   
<suite name="Sequential Suite" parallel="none" data-provider-thread-count="1">
    <test name="Sequential Test">
        <parameter name="Browser" value="chrome"/>
        <classes>
            <class name="com.dsAlgoTestRunner.SequentialAndParallelTestRunner"/>
        </classes>
    </test>
</suite>
    <!-- Parallel Execution Suite -->
    <suite name="Parallel Suite" parallel="methods" data-provider-thread-count="2">
        <test name="Parallel Test">
            <parameter name="Browser" value="chrome"/>
            <classes>
                <class name="com.dsAlgoTestRunner.SequentialAndParallelTestRunner"/>
            </classes>
        </test>
    </suite>
</suite>